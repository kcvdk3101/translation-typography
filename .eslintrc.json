{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": ["react", "jsdoc"],
  "extends": ["plugin:react/recommended", "plugin:jsdoc/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
  }
}
